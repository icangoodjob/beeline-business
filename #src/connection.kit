		<div class="modal fade" id="connection-modal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
			aria-labelledby="connection-modal" aria-hidden="true">
			<div class="modal-dialog modal-dialog-centered">
				<div class="modal-content">
					<button class="modal__close _icon-close" data-bs-dismiss="modal" aria-label="Close">
					</button>
					<form action="#" class="modal__form form-modal">
						<h2 class="form-modal__title">Добавить номер к услуге</h2>
						<div class="form-modal__body">
							<div class="form-modal__item">
								<div class="form-modal__box">
									<div class="form-modal__textarea-wrapper">
										<textarea class="form-modal__textarea" autocomplete="off"
											placeholder="Номера телефонов, через запятую списком"></textarea>
									</div>
									<button class="form-modal__button">Проверить номера</button>
								</div>
							</div>
							<div class="form-modal__item">
								<label class="form-modal__label">Услуга</label>
								<div class="select-item">
									<select name="services" class="form__select">
										<option value="services" selected>Услуга</option>
										<option value="services-2">Речевая аналитика</option>
										<option value="services-2">Речевая аналитика</option>
									</select>
								</div>
							</div>
							<div class="form-modal__item">
								<input class="form-modal__input" type="text" autocomplete="off" placeholder="Описание">
							</div>
						</div>
						<div class="form-modal__buttons">
							<button class="form-modal__button" type="button" data-bs-dismiss="modal"
								aria-label="Close">Отменить</button>
							<button class="form-modal__button form-modal__button_yellow" type="button">Добавить</button>
						</div>
					</form>
				</div>
			</div>
		</div>

		<div class="modal fade" id="connection-redact-modal" data-bs-backdrop="static" data-bs-keyboard="false"
			tabindex="-1" aria-labelledby="connection-redact-modal" aria-hidden="true">
			<div class="modal-dialog modal-dialog-centered">
